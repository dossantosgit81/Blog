                <div class="card-body">
                    <a class="btn btn-success" href="/<%= article.slug %> "><%= article.title  %> </a>
                </div>
            </div>
            <br>
         <% }) %> 
         <div class="row">
             <div class="col col-12">
                 <a href="/articles/page/2" style="float: right;">Next>>>></a>
             </div>
         </div>
